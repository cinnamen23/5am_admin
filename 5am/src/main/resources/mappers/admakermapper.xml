<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.cg.persistence.AdmakerDAO" >


<insert id="adimginsert" parameterType="hashmap">

insert into tbl_adImage(imid,sid)values('${vo.imid}','${sid}') 

</insert>





<insert id="adgifinsert" parameterType="hashmap">

insert into tbl_adgif(gifno,gifname,sid,target1,target2,target3,target4,area)values(seq_adgif_gifno.nextval,'${gifname}','${vo.sid}','${vo.target1}','${vo.target2}','${vo.target3}','${vo.target4}',100) 

</insert>






</mapper>
  
