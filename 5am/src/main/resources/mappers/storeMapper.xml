<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.cg.persistence.StoreDAO">

	<select id="login" resultType="org.cg.domain.StoreVO">
		select sid , spw , sname from tbl_store where sid = #{sid} and spw = #{spw}
	</select>

	<insert id="storeregi">
		INSERT INTO TBL_STORE(sid, spw,sname,saddr,lat,lng,sphone,semail) VALUES('${sid}', '${spw}','${sname}','${saddr}','${lat}','${lng}','${sphone}','${semail}')
	</insert>

</mapper>